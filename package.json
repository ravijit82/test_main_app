{
  "name": "react-starter-template",
  "version": "1.0.0",
  "private": true,
  "bugs": {
    "url": "https://github.com/ServiceXcelerator/xceleratorgroup-react-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ServiceXcelerator/xceleratorgroup-react-template.git"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "dotenv": "^16.0.3",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-intl": "^6.4.2",
    "react-router-dom": "^6.11.0",
    "react-scripts": "5.0.1",
    "sort-by": "^1.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "format": "eslint src --fix",
    "lint": "eslint src",
    "clean": "rm -rf package-lock.json node_modules",
    "extract": "formatjs extract \"src/**/*.{js,jsx}\" --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format simple --out-file generated_locales/en.json",
    "docker": "docker build -t claims-frontend-app . && docker run -d -p 8080:8080 claims-frontend-app"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@formatjs/cli": "^6.1.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8"
  },
  "engines": {
    "npm": ">=9.5.1",
    "node": ">=18.16.0"
  }
}